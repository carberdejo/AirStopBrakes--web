@model IEnumerable<Capa_Entidad.Proveedor>

@{
    ViewBag.Title = "IndexProveedor";
}

<h2>IndexProveedor</h2>

<p>
    @Html.ActionLink("Crear", "CreateProveedor")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cod_prov)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nom_prov)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pais_origen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telefono)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cod_prov)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nom_prov)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pais_origen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.telefono)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditProveedor", new { id=item.cod_prov }) |
            @Html.ActionLink("Details", "DetailsProveedor", new {  id=item.cod_prov }) |
            @Html.ActionLink("Delete", "DeleteProveedor", new {  id=item.cod_prov })
        </td>
    </tr>
}

</table>
